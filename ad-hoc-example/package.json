    "@xdn/prefetch": "file:.yalc/@xdn/prefetch",
    "compression": "^1.7.4",
    "polka": "^0.5.2",
    "sapper": "^0.28.0",
    "sirv": "^1.0.7",
    "svelte": "^3.17.3",
    "svelte-loader": "^2.9.0",
    "webpack": "^4.7.0",
    "webpack-modules": "^1.0.0"
  }
}
